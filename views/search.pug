extends layouts/main
include mixins/videoBlock

block content
    .search__header
        h3 Searching for: #{searching}
    .search__result
        if videos.length === 0
            h5 No Videos Found
        each video in videos
            +videoBlock({
                videoFile:video.videoFile,
                title:video.title,
                views:video.views,
                id:video.id
            })

